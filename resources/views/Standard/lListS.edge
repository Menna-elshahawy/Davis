                          </div>


                           <div id="remHM" class="remHM">
                               <div id="rh1" class="rh1">  public Object removeFirst() { </div> 
                               <div id="rh2" class="rh2" style="text-indent: 40px;">  Object res = head.data; </div> 
                               <div id="rh3" class="rh3" style="text-indent: 40px;">  head = head.next; </div> 
                               <div id="rh4" class="rh4" style="text-indent: 40px;">  return res; </div>
                               <div id="rh5" class="rh5">  }</div> 
                          </div>

                           <div id="remTM" class="remTM">
                               <div id="rt1" class="rt1">  public void removeLast() { </div> 
                               <div id="rt2" class="rt2" style="text-indent: 40px;">  if (head.next == null) { </div> 
                               <div id="rt3" class="rt3" style="text-indent: 80px;">  Object res = head.data; </div> 
                               <div id="rt4" class="rt4" style="text-indent: 80px;">  head = null; </div>
                               <div id="rt5" class="rt5" style="text-indent: 80px;">  return res; </div>
                               <div id="rt6" class="rt6" style="text-indent: 40px;">  } </div>
                               <div id="rt7" class="rt7" style="text-indent: 40px;">  Link current = head; </div>
                               <div id="rt8" class="rt8" style="text-indent: 40px;">  while (current.next.next != null) </div>
                               <div id="rt9" class="rt9" style="text-indent: 80px;">  current = current.next; </div>
                               <div id="rt10" class="rt10" style="text-indent: 40px;">  Object res = current.next.data; </div>
                               <div id="rt11" class="rt11" style="text-indent: 40px;">  current.next = null; </div>
                               <div id="rt12" class="rt12" style="text-indent: 40px;">  return res; </div>
                               <div id="rt13" class="rt13">  }</div> 
                          </div>

                          <div id="srchM" class="srchM">
                               <div id="s1" class="s1">  public String Search(Object v){  </div> 
                               <div id="s2" class="s2" style="text-indent: 40px;">  Link current = head; </div> 
                               <div id="s3" class="s3" style="text-indent: 40px;">  while (current != null){ </div> 
                               <div id="s4" class="s4" style="text-indent: 80px;">  if(current.equals(v)) </div> 
                               <div id="s5" class="s5" style="text-indent: 120px;">  return "found";</div> 
                               <div id="s6" class="s6" style="text-indent: 80px;">  current = current.next;  </div> 
                               <div id="s7" class="s7" style="text-indent: 40px;">  } </div> 
                               <div id="s8" class="s8" style="text-indent: 40px;">  return "not found"; </div> 
                               <div id="s9" class="s9">  }</div> 
                          </div>
                           
