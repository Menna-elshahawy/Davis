                    document.getElementById("notify").innerHTML= "";
                    
                    var l=document.getElementById("size").value;

                    if(l>7){
                        document.getElementById("notify").innerHTML= "Maximum size is 7";
                        document.getElementById("size").value="";

                    }
                    else{
                        document.getElementById("notify").innerHTML= "";
                        document.getElementById("size").value="";
                        
                        list=[];
                        for(i=0;i<l;i++){
                            
                            do{
                                var x = Math.floor((Math.random() * 9) + 1);
                            }while(exist(x));
                            
                            list[i]=x;    
                        }
                        History.push("Create random linked list: "+list)
                        this.display();
                    }
                    
                
