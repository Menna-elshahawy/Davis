                    var ar=document.getElementById("arr").value;
                    document.getElementById("arr").value="";
                    
                    if(ar==""){
                    document.getElementById("arr").value="";   
                    }
                    
                    else{
                        var list2=[];

                        list2=ar.split(',');    

                        if(list2.length>7){
                            document.getElementById("notify").innerHTML= "Maximum size is 8";
                            document.getElementById("arr").value="";

                        }
                        else{
                            list=[];
                            var f='f';
                            
                            for(i=0;i<list2.length;i++){
                                if(exist(list2[i])){
                                    f='t';
                                    break;
                                }
                                else{
                                    list[i]=list2[i];
                                }
                            }
                            
                            if(f=='f'){
                                History.push("Create user-defined linked list: "+list)
                                this.display();
                            }
                            else{
                                document.getElementById("notify").innerHTML= "No duplicate values allowed";
                                document.getElementById("arr").value="";
                            }
                    } 
